{
  "name": "directus-extension-expandable-blocks-api",
  "version": "0.1.0",
  "description": "API endpoint extension for expandable blocks",
  "author": "Christopher Schwarz <christopher@neugebauerschwarz.com>",
  "license": "MIT",
  "type": "module",
  "files": [
